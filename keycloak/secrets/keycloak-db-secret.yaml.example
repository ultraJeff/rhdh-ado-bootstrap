# Template for keycloak-db-secret.yaml
# Copy this file to keycloak-db-secret.yaml and fill in your credentials
# The actual secret file is gitignored for security
apiVersion: v1
kind: Secret
metadata:
  name: keycloak-db-secret
  namespace: keycloak
type: Opaque
stringData:
  username: <your-postgres-username>
  password: <your-postgres-password>
